{
  "name": "AWS-Examples",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:1": {}
  },

  "containerEnv": {
    "AWS_CLI_AUTO_PROMPT": "on-partial"
  },
  
 "postCreateCommand": "sudo apt-get update && sudo apt-get install -y tree",
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "Frontend",
      "onAutoForward": "openPreview"
    }
  }
}